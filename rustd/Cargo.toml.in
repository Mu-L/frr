[package]
name = "rustd"
version = "0.1.0"
edition = "2021"
default-run = "rustd"

# [build]
# target = @abs_builddir@

[[bin]]
name = "rustd"
test = false
bench = false
path = "@srcdir@/rust_main.rs"

[dependencies]
tracing = "0.1.40"
tracing-subscriber = { version = "0.3.18", features = ["env-filter"]}

[build-dependencies]
bindgen = "0.70.1"
cc = "1.1.18"
